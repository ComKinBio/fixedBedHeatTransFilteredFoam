        rho             cell;
        U               cellPoint;
        thermo:mu       cell;
        T               cell;
        Cp              cell;
        kappa           cell;
        p               cell;
        G               cell;
        alpha           cell;
        rhoavg          cell;
        Uavg            cell;
        muavg           cell;
        Tavg            cell;
        cpavg           cell;
        kappaavg        cell;
    }

    integrationSchemes
    {
        U               Euler;
        T               analytical;
    }
}

